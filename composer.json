{
    "name": "ise/ise-module-api",
    "description": "API module for Zend Framework 2 used by Internalsystemerror Limited",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/ise-module-api",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-eventmanager": "^2.5",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-mvc": "^2.5"
    },
    "require-dev": {
        "ise/php-metapackage-development": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IseApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IseApiTest\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs",
            "@phpmd",
            "@phpcpd"
        ],
        "phpunit": "phpunit -c ./phpunit.xml.dist",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpmd": "phpmd . html --reportfile ./build/logs/phpmd.html --suffixes php,phtml --exclude vendor,build ./phpmd.xml.dist",
        "phpcpd": "phpcpd . --exclude=vendor --exclude=data"
    }
}
